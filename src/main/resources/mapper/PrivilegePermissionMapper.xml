<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yzg.gsms.dao.PrivilegePermissionMapper">
    <sql id="tableName">
      ${dbSchema}privilege_permission
    </sql>

    <insert id="insert" parameterType="PrivilegePermission">
        INSERT INTO
        <include refid="tableName"/>
        (id,code,name,is_enabled,
        creator,create_time,last_modifier,last_modifier_time,
        sequence)
        VALUES(#{id},#{code},#{name},#{isEnabled},
        #{creator},#{createTime},#{lastModifier},#{lastModifierTime},
        #{sequence})
    </insert>

    <select id="get" parameterType="String" resultType="PrivilegePermission">
        SELECT *
        FROM
        <include refid="tableName"/>
        WHERE id = #{id}
    </select>

    <update id="update" parameterType="PrivilegePermission">
        UPDATE
        <include refid="tableName"/>
        SET code = #{code},name = #{name},is_enabled = #{isEnabled},
        last_modifier = #{lastModifier},last_modifier_time = #{lastModifierTime},
        sequence = #{sequence}
        WHERE id = #{id}
    </update>

    <update id="logicalDelete" parameterType="PrivilegePermission">
        UPDATE
        <include refid="tableName"/>
        SET is_enabled = 0,
        last_modifier = #{lastModifier},last_modifier_time = #{lastModifierTime}
        WHERE id = #{id}
    </update>

    <delete id="delete" parameterType="String">
        DELETE FROM
        <include refid="tableName"/>
        WHERE id = #{id}
    </delete>
</mapper>